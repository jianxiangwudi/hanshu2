//求第n个斐波那契数


#include <stdio.h>
int qiuhe(int x)
{
	if (x >=3)
		return qiuhe(x - 1) + qiuhe(x - 2);
	else
		return 1;

}
int main()
{
	int fib = 0;
	int i = 0;
	scanf("%d", &i);

	fib = qiuhe(i);
	printf("%d\n", fib);
	return 0;
}



